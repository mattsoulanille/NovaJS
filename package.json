    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "browserify": "^16.2.2",
    "watchify": "^3.11.0",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "build": "mkdir -p ./static/ && browserify -g uglifyify lib/browser.js | uglifyjs -c > static/bundle.js",
    "watch": "mkdir -p ./static/ && watchify lib/browser.js -o static/bundle.js --debug --verbose",
    "run": "node index.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattsoulanille/NovaJS.git"
  },
