{
  "name": "novajs",
  "version": "0.6.0",
  "description": "An implementation of EV Nova in the browser",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "crash-colliders": "^2.0.1",
    "express": "^4.16.3",
    "novaparse": "^0.0.42",
